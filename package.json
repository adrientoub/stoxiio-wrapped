{
  "name": "finance-wrapped",
  "version": "1.0.0",
  "description": "Spotify Wrapped-style yearly finance summary",
  "scripts": {
    "proxy": "node proxy.js",
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "concurrently \"npm run proxy\" \"npm run dev\"",
    "format": "prettier --write \"src/**/*.ts\" \"*.js\" \"*.json\" \"*.css\" \"*.html\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"*.js\" \"*.json\" \"*.css\" \"*.html\""
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "node-fetch": "^2.7.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "concurrently": "^8.2.0",
    "prettier": "^3.7.4",
    "typescript": "^5.3.0"
  }
}
